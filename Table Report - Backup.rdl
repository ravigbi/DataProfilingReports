    <DataSet Name="dsTableInfo">
      <Query>
        <DataSourceName>Localhost</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pDatabaseName">
            <Value>=Parameters!pDatabaseName.Label</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-------------------------------------
--Declarations
declare @dbname sysname, @ssql nvarchar(max), @stmp char(7)

-------------------------------------
--Initialization
SET @dbname = @pDatabaseName
SET @stmp = 'OwnerID'

set @ssql = '
select  DB_ID(''' + @dbname + ''') as Database_id, 
obj.object_id, 
sch.name as SchemaName, 
obj.name as TableName, 
obj.type_desc as TableType, 
obj.create_date as CreateDate, 
obj.modify_date as LastModifiedDate,  
rcnt.[Rows] as [RowCount], 
ISNULL(cols.ColumnCount, 0) as [ColumnCount], 
ISNULL(indx.IndexCount, 0) as [IndexCount], (Pages * 8) as DataSize, 
CASE WHEN usedPages &gt; Pages THEN (usedPages - Pages) * 8  ELSE 0 END as [IndexSize], 
USER_NAME(cast(OBJECTPROPERTYEX(obj.object_id, N''' + @stmp + ''') as int)) as [OwnerName], 
CASE WHEN ISNULL(pkey.pkeys, 0) &gt; 0 THEN ''Y'' ELSE ''N'' END as HasPrimaryKey, 
CASE WHEN ISNULL(cols.IdentityCount, 0) &gt; 0 THEN ''Y'' ELSE ''N'' END as HasIdentityColumn, 
ISNULL(Fkey.Fkeys, 0) as ForeignKeys 
from ' + quotename(@dbname) + '.sys.objects obj 
inner join ' + quotename(@dbname) + '.sys.tables tbl ON obj.object_id = tbl.object_id 
inner join ' + quotename(@dbname) + '.sys.schemas sch ON sch.schema_id = obj.schema_id 
inner join  
(
	SELECT  	
	object_id,  	
	SUM (used_page_count) as usedPages, 	
	SUM ( CASE WHEN (index_id &lt; 2) THEN (in_row_data_page_count + lob_used_page_count + row_overflow_used_page_count) 	ELSE 0 	END ) as Pages, 	
	SUM (CASE WHEN (index_id &lt; 2) THEN row_count ELSE 0 END ) as [Rows] 	
	FROM ' + quotename(@dbname) + '.sys.dm_db_partition_stats 	
	group by object_id 
) as rcnt 
ON rcnt.object_id = obj.object_id  

left outer join  
(
	select object_id, 
	count(*) ColumnCount, 
	max(cast(is_Identity as int)) as IdentityCount 
	from ' + quotename(@dbname) + '.sys.all_columns 
	group by object_id 
) as cols 
ON cols.object_id = obj.object_id  

left outer join 
( 
	select  object_id, 
	count(*) IndexCount 
	from ' + quotename(@dbname) + '.sys.indexes 
	where type IN (1,2) 
	group by object_id 
) as indx 
ON indx.object_id = obj.object_id 

left outer join  
( 
	select parent_object_id, 
	count(*) as Pkeys 
	from ' + quotename(@dbname) + '.sys.key_constraints   
	where type = ''PK''  
	group by parent_object_id 
) as pkey 
ON pkey.parent_object_id = obj.object_id 
left outer join 
( 
	select parent_object_id, 
	count(*) as Fkeys 
	from ' + quotename(@dbname) + '.sys.foreign_keys   
	where type = ''F'' 
	group by parent_object_id 
) as fkey 
ON fkey.parent_object_id= obj.object_id 
WHERE obj.type = ''U''
'
--select @ssql

EXEC(@ssql)</CommandText>

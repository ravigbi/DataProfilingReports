        <CommandText>-------------------------------------
--Declarations
declare @obid int, @dbname sysname, @schemaname sysname, @tablename sysname

declare @colcount int, @cntr int 
declare @ssql nvarchar(max)

declare @colname sysname, @colid int, @datatypename varchar(50), @tobjid int


-------------------------------------
--Initialization
--SET @obid = 373576369
SET @dbname = @pDatabaseName
SET @schemaname = @pSchemaName
SET @tablename = @pTableName

SET @cntr = 1

--------------------------------------------
--Temp Table creation
if(OBJECT_id(N'tempdb..#tablebase', N'U') is not null) drop table tempdb..#tablebase
if(OBJECT_id(N'tempdb..#tablestats', N'U') is not null) drop table tempdb..#tablestats

create table #tablebase
(
object_id int not null,
SchemaName sysname not null,
TableName sysname not null,
column_id int not null,
ColumnName sysname not null,
IsIdentity bit not null,
IsNullable bit not null,
user_type_id int not null,
DateTypeName sysname not null,
precision int not null,
scale int not null,
max_length int not null,
Collation varchar(50),
CreateDate datetime
)

create table #tablestats
(
object_id int not null,
column_id int not null,
NullValues int,
DistinctValues int,
BlankValues int,
ZeroRows int,
MinValue varchar(max),
MaxValue varchar(max),
MaxLenght int
)


SET @ssql = '
INSERT INTO #TABLEBASE 
select 
obj.object_id, sch.name as SchemaName, obj.name as TableName, cls.column_id, cls.name ColumnName, 
cls.is_identity IsIdentity, cls.is_nullable as IsNullable,
cls.user_type_id, tps.name DataTypeName, tps.precision, tps.scale, cls.max_length, tps.collation_name as Collation, obj.create_date as CreateDate
from ' + quotename(@dbname) + '.sys.objects obj
inner join ' + quotename(@dbname) + '.sys.tables tbl ON obj.object_id = tbl.object_id
inner join ' + quotename(@dbname) + '.sys.schemas sch ON sch.schema_id = obj.schema_id
inner join ' + quotename(@dbname) + '.sys.columns cls ON cls.object_id = obj.object_id
inner join ' + quotename(@dbname) + '.sys.types tps ON cls.user_type_id = tps.user_type_id
WHERE obj.type = ''U''
AND obj.name = ''' + @tablename + '''
AND sch.name = ''' + @schemaname + '''
Order by sch.name, obj.object_id
'

EXEC(@ssql)
SET @colcount = @@ROWCOUNT

--select @ssql
--select * from #tablebase

--SET @ssql = ''

while @cntr &lt;= @colcount

begin

select @tobjid = object_id, @datatypename = DateTypeName, @colname = ColumnName, @colid = column_id
from #tablebase
Where column_id = @cntr
--print @tobjid

-- Character data types 
	if @datatypename IN ('varchar', 'nvarchar', 'char', 'nchar', 'sysname')
	select @ssql = N'INSERT #tablestats
	SELECT ' + CAST(@tobjid as varchar(20)) + ', ' + cast(@colid as varchar(4)) + ', SUM(CASE WHEN ' + @colname + ' IS NULL THEN 1 ELSE 0 END),
	COUNT(DISTINCT ' + @colname + '), SUM(CASE WHEN ' + @colname + ' = '''' THEN 1 ELSE 0 END), 0,
	MIN(CAST(' + @colname + ' as varchar(50))), MAX(CAST(' + @colname + ' as varchar(50))), LEN(MAX(CAST(' + @colname + ' as varchar(50))))
	from ' + quotename(@dbname) + '.' + quotename(@schemaname) + '.' + quotename(@tablename)

	else 
	begin -- integer data types 
		if @datatypename IN ('int', 'bigint', 'tinyint', 'smallint', 'real', 'money', 'smallmoney', 'decimal', 'numeric', 'float')
		select @ssql = N'INSERT #tablestats
		SELECT ' + CAST(@tobjid as varchar(20)) + ', ' + cast(@colid as varchar(4)) + ', SUM(CASE WHEN ' + @colname + ' IS NULL THEN 1 ELSE 0 END),
		COUNT(DISTINCT ' + @colname + '), 0, SUM(CASE WHEN ' + @colname + ' = 0 THEN 1 ELSE 0 END),
		MIN(' + @colname + '), MAX(' + @colname + '), 0
		from ' + quotename(@dbname) + '.' + quotename(@schemaname) + '.' + quotename(@tablename)

		else
		begin -- Date data types 
			if @datatypename IN ('datetime', 'date', 'time', 'datetime2', 'smalldatetime', 'timestamp', 'datetimeoffset')
			select @ssql = N'INSERT #tablestats
			SELECT ' + CAST(@tobjid as varchar(20)) + ', ' + cast(@colid as varchar(4)) + ', SUM(CASE WHEN ' + @colname + ' IS NULL THEN 1 ELSE 0 END),
			COUNT(DISTINCT ' + @colname + '), 0, 0,
			MIN(' + @colname + '), MAX(' + @colname + '), 0
			from ' + quotename(@dbname) + '.' + quotename(@schemaname) + '.' + quotename(@tablename)

			else -- All unsupported or special data types - bit, uniqueidenfier, xml, text, ntext, binary, varbinary and other types
			select @ssql = N'INSERT #tablestats
			SELECT ' + CAST(@tobjid as varchar(20)) + ', ' + cast(@colid as varchar(4)) + ', SUM(CASE WHEN ' + @colname + ' IS NULL THEN 1 ELSE 0 END),
			0, 0, 0, '''', '''', NULL
			from ' + quotename(@dbname) + '.' + quotename(@schemaname) + '.' + quotename(@tablename)
		end

	end

--select @tobjid, CAST(@tobjid as varchar(20)), @datatypename, @colname, @colid, @cntr, @ssql

exec(@ssql) 

SET @cntr+= 1
--SET @ssql = ''
end --while loop


select bs.ColumnName, bs.IsIdentity, bs.IsNullable, bs.DateTypeName, bs.precision, bs.scale, bs.max_length, bs.Collation, bs.CreateDate,
st.NullValues, st.DistinctValues, st.BlankValues, st.ZeroRows, st.MinValue, st.MaxValue, st.MaxLenght 
from #tablebase bs
left outer join #tablestats st 
ON bs.object_id = st.object_id AND bs.column_id = st.column_id</CommandText>
      </Query>

        <CommandText>=" select  idx.object_id, idx.index_id, icols.key_ordinal, obj.name as TableName, idx.name as IndexName, idx.type_desc as IndexType,  CASE WHEN idx.is_primary_key = 1 then 'PRIMARY' ELSE '' END as is_primary_key, CASE WHEN idx.is_unique = 1 THEN 'UNIQUE' ELSE '' END as is_unique, cols.name as ColumnName, CASE WHEN icols.is_descending_key = 1 THEN 'DESC' ELSE 'ASC' END as is_descending_key from " &amp;Parameters!pDatabaseName.Label &amp; ".sys.indexes idx inner join " &amp;Parameters!pDatabaseName.Label &amp; ".sys.objects obj ON idx.object_id = obj.object_id inner join " &amp;Parameters!pDatabaseName.Label &amp; ".sys.index_columns icols ON idx.object_id = icols.object_id AND idx.index_id = icols.index_id inner join " &amp;Parameters!pDatabaseName.Label &amp; ".sys.columns cols ON cols.object_id = idx.object_id AND cols.column_id = icols.column_id where idx.type &gt; 0 AND obj.type = 'U' AND idx.object_id = " &amp; Parameters!pTableName.Value</CommandText>

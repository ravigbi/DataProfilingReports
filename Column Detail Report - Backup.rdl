SET @ssql = 'SELECT TOP 10 ' + quotename(@colname) + ' as ColumnValue, count(*) as RowCounts
FROM ' + quotename(@dbname) + '.' + quotename(@schemaname) + '.' + quotename(@tablename) + '
GROUP BY ' + quotename(@colname) + '
ORDER BY RowCounts desc
'
--select @ssql, len(@ssql)

EXEC(@ssql)</CommandText>

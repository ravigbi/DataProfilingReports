IF object_id('tempdb..#tempcoldetails') is not null drop table #tempcoldetails
CREATE TABLE #tempcoldetails
(
ColumnValue varchar(50),
RowCounts int,
RowPercentage decimal(10,2)
)


SET @ssql = 'SELECT ' + @colname + ' as ColumnValue, count(*) as RowCounts, 
Round((count(*) * 100.0/' + cast(@rowcount as varchar(50)) + '), 2) as RowPercentage
FROM ' + @dbname + '.' + @schemaname + '.' + @tablename + '
GROUP BY ' +  @colname

--select @ssql
INSERT INTO #tempcoldetails
EXEC(@ssql)


select '0-10' as ValueRange, count(*) RowCounts
from #tempcoldetails
where RowPercentage between 0 AND 10
union
select '10-20' as ValueRange, count(*) RowCounts
from #tempcoldetails
where RowPercentage between 11 AND 20
union
select '20-30' as ValueRange, count(*) RowCounts
from #tempcoldetails
where RowPercentage between 21 AND 30
union
select '30-40' as ValueRange, count(*) RowCounts
from #tempcoldetails
where RowPercentage between 31 AND 40
union
select '40-50' as ValueRange, count(*) RowCounts
from #tempcoldetails
where RowPercentage between 41 AND 50
union
select '50-60' as ValueRange, count(*) RowCounts
from #tempcoldetails
where RowPercentage between 51 AND 60
union
select '60-70' as ValueRange, count(*) RowCounts
from #tempcoldetails
where RowPercentage between 61 AND 70
union
select '70-80' as ValueRange, count(*) RowCounts
from #tempcoldetails
where RowPercentage between 71 AND 80
union
select '80-90' as ValueRange, count(*) RowCounts
from #tempcoldetails
where RowPercentage between 81 AND 90
union
select '90-100' as ValueRange, count(*) RowCounts
from #tempcoldetails
where RowPercentage between 91 AND 100</CommandText>
